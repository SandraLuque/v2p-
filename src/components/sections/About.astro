---
import { getI18N } from "@/i18n";
import MainSection from "./MainSection.astro";
import Title from "./Title.astro";
const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<MainSection id="sobre-mi" type="container" class="pb-32">
  <Title text={i18n.SECTION_ABOUT_TITLE} />
  <p class="text-base md:text-mediun" set:html={i18n.SECTION_ABOUT_SUBTITLE} />
</MainSection>
